pico-8 cartridge // http://www.pico-8.com
version 32
__lua__
function _init()
 py=0
 x,y,ox,oy=64,64,64,64
 smoke,fire=false,false
 boost,beam=127,127
 dc,dct,dcy=0,0,0
 dh,dht,dhy,dhx,ohy,ohx=0,0,0,0,0,0
 rectfill(0,0,127,127,12)
 title=2
 hk,bk=0,0
 dp,dy=0,0 
 ddialog=false
 music(0)
end

function _update()
 py+=1
 py%=128
 if (title>0) then
  if (btnp(5) or btnp(4)) then
   title=1
   music(-1)
  end
  return
 end
 menuitem(1,"what's going on?",thehaps)
 if (btn(5)) fire=true else fire=false
 if (btn(4)) smoke=true else smoke=false
 if (smoke and boost>0) then
  boost-=1
  if (btn(0)) x-=5
  if (btn(1)) x+=5
  if (btn(2)) y-=5
  if (btn(3)) y+=5
  sfx(1,3)
 else 
  if (btn(0)) x-=3
  if (btn(1)) x+=3
  if (btn(2)) y-=3
  if (btn(3)) y+=3
  sfx(0,3)
 end
 if (not smoke and boost<127) boost+=1
 if (fire and beam>0) then
  beam-=1
  sfx(2,2)
 else
  sfx(-2,2)
 end
 if (not fire and beam<127) beam+=1
 if (x<0) x=0
 if (x>120) x=120
 if (y<0) y=0
 if (y>120) y=120
 if (dc==0) then
  local z=flr(rnd(10))
  if (z==0) then
   dc=1
   dcy=rnd(90)+10
  end
 end
 if (dh>0) then
  if (dhy>y&126) then
    dhy-=2
  elseif (dhy<y&126) then
    dhy+=2
  end
  if (flr(rnd(3))>0) dhx-=2
  if (dhx<=-8) dh=0
  if ((fire and beam>0 and y>=dhy-4 and y<=dhy+6 and x<=dhx-7) 
    or (y>=dhy-4 and y<=dhy+6 and x>=dhx-14 and x<=dhx-8)) then
   dh=-1
   if dht==1 then
    bk+=1
    sfx(3)
   else
    hk+=1
    sfx(4)
   end
  end
 end
 if (dh==0) then
  if (flr(rnd(10))==0) then
   dh=1
   dhx=120
   dhy=0
   if (flr(rnd(2))==0) dhy=120
  end
 end
end

function _draw()
 if (title==2) then
  cls()
--  rectfill(0,0,127,127,12)
  spr(128,0,16,16,8)
  print("\^w\^tgrand",48,28,2)
  print("\^w\^tcloud",48,44,2)
  print("\^w\^talley",48,60,2)
  print("for rndgame jam ii  #",0,0,6)
  s="rndgame2021"
  for i=1,#s do
   print(sub(s,i,i),(i+20)*4,0,flr(rnd(15))+1)
  end
  print("by impliedchaos",36,84,5)
  s="press 🅾️  or ❎  to start"
--  ,20,100,7)
  for i=1,#s do
   print(sub(s,i,i),16+i*4,108+sin(i/20+(py&63)/63+0.25)*3,7)
  end
  return
 elseif (title==1) then
  rectfill(0,0,127,127,12)
 end
 -- erase our sprites
 if title==0 then
  spr(128,0,oy,16,1)
  spr(192,0,8,16,1)
  spr(208,0,112,16,1)
  if (dh!=0 and dh!=1) spr(144,ohx,ohy)
 end
 title=0
 
 -- draw 2 pixels of sky
 rect(0,0,1,127,12)

 -- draw oncoming clouds
 if (dc>0) then
   local dx=dc-1
   spr(96+dct*4,0-dx*2,dcy,dc/4,2)
   dc+=1
   if (dc>16) then
     dc=0
     dct+=1
     dct%=4
   end
 end

 -- shift screen left 2px
 for i=1,126 do
  memcpy(0x6000+i*64,0x6001+i*64,64)
 end
 line(0,0,127,0,12)
 line(0,127,127,127,12)
 
 -- draw smoke cloud
 local z=1
 local s=rnd(4)+1
 if (smoke and boost>0) s=rnd(8)+1
 if (py&1==0) z=-1
 circ(x-4,y+4,s,6)
 circfill(x-4,y+4+z,s,7)

 if (dh==-1) then
  splodey(dhx,dhy)
  dh=0
 end

 -- save screen to erase sprites on next pass
 memcpy(0x1000,0x6000+y*64,512)
 memcpy(0x1800,0x6200,512)
 memcpy(0x1a00,0x7c00,512)
 if (dh>0) then
  for i=0,7 do
   memcpy(0x1200+i*64,0x6000+(dhy+i)*64+dhx\2,4)
 --   cstore(0x1200+i*64,0x6000+dhy*64+dhx/2,4)
  end
   ohx,ohy=dhx,dhy
 end
 
 -- draw halucination
 if (dh>0) then
  dht=1
  dh+=1
  local hf=(py&4) >> 2
  spr(20+dht*2+hf,dhx,dhy)
 end
 
 -- draw ship
 local frame=py&7
 spr(2+frame*2,x,y,2,1)
 ox,oy=x,y
 -- beam cannon
 if (fire and beam>0) then
   local fr2=py&3
   local pos=x+16
   if (pos<128) then
     spr(64+fr2,pos,y)
     pos+=8
     while (pos<127) do
       spr(80+fr2,pos,y)
       pos+=8
     end
   end
 end

 -- draw cloud alley
 spr(32,py*-1,112,16,2)
 spr(32,128-py,112,16,2)
 spr(32,py*-1,0,16,2,true,true)
 spr(32,128-py,0,16,2,true,true)

 -- draw gauges
 rect(9,1,42,7,3)
 rect(84,1,117,7,2)
 rectfill(10,2,41,6,0x49)
 rectfill(85,2,116,6,0x49)
 if (boost>0) rectfill(10,2,boost/4+10,6,11)
 if (smoke) print("🅾️boost",12,2,7) else print("🅾️boost",12,2,0)
 if (beam>0) rectfill(85,2,beam/4+85,6,8)
 if (fire) print("❎beam",89,2,7) else print("❎beam",89,2,0)

 local s="-=] grand cloud alley [=-"
 for i=1,#s do
  print(sub(s,i,i),10+i*4,120+sin(i/20+(py&63)/63+0.25)*3,12)
  print(sub(s,i,i),10+i*4,120+sin(i/20+(py&63)/63+0.2)*3,11)
  print(sub(s,i,i),10+i*4,120+sin(i/20+(py&63)/63+0.15)*3,10)
  print(sub(s,i,i),10+i*4,120+sin(i/20+(py&63)/63+0.1)*3,9)
  print(sub(s,i,i),10+i*4,120+sin(i/20+(py&63)/63+0.05)*3,8)
  print(sub(s,i,i),10+i*4,120+sin(i/20+(py&63)/63)*3,0)
 end
end

function splodey(x,y)
 x+=4
 y+=4
 circ(x,y,5,6)
 circfill(x,y,4,7)
 for i=0,6 do
  local r=flr(rnd(5))+1
  local ox=1
  local oy=1
  if (flr(rnd(2))==0) ox=-1 
  if (flr(rnd(2))==0) oy=-1 
  local t=rnd(1)
  circ(x+ox+sin(t)*r,y+oy+cos(t)*r,r,6)
  circfill(x+sin(t)*r,y+cos(t)*r,r,7)
 end   
end

function thehaps()
end
-->8
dialog={
 { m="what's going on?",
   t={
   "welcome to the grand cloud alley",
   "",
   "you are an automated drone",
   "designed to protect this passage",
   "from all threats...",
   "...when there were some.",
   },
   options={
    { t="cool, i'm gonna kill stuff.", d=1},
    { t="wait, what?", d=1 }
   }
 },
}

function dmenu(x)
 dy=0
 dp=x
 
end

function ddraw(x)
end
