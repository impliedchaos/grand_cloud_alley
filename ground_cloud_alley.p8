pico-8 cartridge // http://www.pico-8.com
version 32
__lua__
function _init()
 py=0
 x,y,ox,oy=64,64,64,64
 smoke,fire=false,false
 boost,beam=127,127
 dc,dct,dcy=0,0,0
 dh,dht,dhy,dhx,ohy,ohx=0,0,0,0,0,0
 rectfill(0,0,127,127,12)
 title=2
 hk,bk,bs,streak=0,0,0,0
 dp,dy=0,0 
 ddialog,dend=false,false
 music(0)
 hf=10
 fetti={"evil","psycho","killer","murderer","crazy","nutter","killer"}
 text={
  "the grand cloud alley",
  "",
  "the sky passage used as a",
  "shipping lane for the cloud",
  "empire...when there was one.",
  "",
  "you are automated defense drone",
  "xkh-33221637, first commisioned",
  "41,282 years ago.",
  "",
  "you have been tasked with",
  "defending the alley from evil",
  "invaders...when there were any.",
  "",
  "now you just fly around doing",
  "nothing.",
 }
end

function _update()
 if ddialog then
  sfx(-2,2)
  sfx(-2,3)
  if (dy<2048) dy+=1 else dy=1025
  if (dp==1) then 
   text={
    "uh oh. have you started going",
    "insane too?",
    "",
    "you've been halucinating.",
    "",
    "there are no enemies out there",
    "anymore, just birds.  more",
    "specifically: waterfowl.",
    "",
    "for some reason they are",
    "attracted to the sound of your",
    "engines...",
   }
  elseif (dp==2) then 
   text={
    "how would i know?",
    "",
    "i'm just an automated",
    "subroutine...i think.",
    "",
    "maybe try to fly away from",
    "them?",
    "",
    "then again, you've probably",
    "already killed thousands of",
    "them and they just keep flying",
    "straight towards you.",
    "",
    "maybe the only option is to",
    "power down.",
   }
  elseif dp==3 then
   text={
    "congratulations!",
    "",
    "you've managed to spare enough",
    "ducks that you inspired me to",
    "shutdown all of the drones and",
    "let the ducks live in peace.",
    "",
    "goodbye, xkh-33221637, and may",
    "you find peace in the void.",
   }
  end
  sz=0
  for i=1,#text do
   sz+=#text[i]
  end
  if (dy<sz) sfx(5)
  if (btnp(4) or btnp(5)) then
   if dy<sz then
    dy=1024
    return
   end
   if dp==0 then
    dp,dy=1,0
    return
   else 
    if dp==1 then
     dp,dy=2,0
     menuitem(1)
     menuitem(1,"what do i do?",helpme)
    elseif dp==3 then
     reload(0x1000,0x1000,4096)
     pal()
     _init()
     return
    end
    dend=true
    dht=1
   end
  else
   return
  end
 end
 py+=1
 py%=128
 if (title>0) then
  if (btnp(5) or btnp(4)) then
   title=1
   music(-1)
  end
  return
 end
 if (dp==0) menuitem(1,"what's going on?",thehaps)
 if (btn(5)) fire=true else fire=false
 if (btn(4)) smoke=true else smoke=false
 if (smoke and boost>0) then
  boost-=1
  if (btn(0)) x-=5
  if (btn(1)) x+=5
  if (btn(2)) y-=5
  if (btn(3)) y+=5
  sfx(1,3)
 else 
  if (btn(0)) x-=3
  if (btn(1)) x+=3
  if (btn(2)) y-=3
  if (btn(3)) y+=3
  sfx(0,3)
 end
 if (not smoke and boost<127) boost+=1
 if (fire and beam>0) then
  beam-=1
  sfx(2,2)
 else
  sfx(-2,2)
 end
 if (not fire and beam<127) beam+=1
 if (x<0) x=0
 if (x>120) x=120
 if (y<0) y=0
 if (y>120) y=120
 if (dc==0) then
  if (rand(10)==0) then
   dc=1
   dcy=rand(90)+10
  end
 end
 if (dh>0) then
  if (dhx>=x or dhy<=20 or dhy>=100) then
   if (dhy>y&126) then
     dhy-=2
   elseif (dhy<y&126) then
     dhy+=2
   end
  end
  if (smoke and boost>0) then
   dhx-=4
  else
   if (rand(3)>0) dhx-=2
  end
  if (dhx<=-8) then
   dh=0
   if (dht==1) then
    sfx(6)
    bs+=1
    streak+=1
   end
  end
  if (dh>0 and ((fire and beam>0 and y>=dhy-4 and y<=dhy+6 and x<=dhx-7) 
    or (y>=dhy-4 and y<=dhy+6 and x>=dhx-14 and x<=dhx-8))) then
   dh=-1
   if dht==1 then
    bk+=1
    sfx(3)
    streak=0
   else
    hk+=1
    sfx(4)
   end
  end
 end
 if (dh==0) then
  if (rand(hf)==0) then
   dh=1
   dhx=120
   dhy=0
   if (rand(2)==0) dhy=120
  end
 end
 if (bs>=20 and (bk/bs<=1 or streak==75)) then
  dp,dy,ddialog=3,0,true
 end 
end

function _draw()
 if ddialog then
  cls()
  if (not dend) then
   local cz=dy
   for i=1,#text do
    for j=1,#text[i] do
     print(sub(text[i],j,j),j*4,i*6,7)
     cz-=1
     if (cz==0) break
    end
    if (cz==0) break
   end
   if dy>sz then
    if dp==0 then 
     print("\^iwait, what?",10,112)
    elseif dp==3 then
     print("\^ithe end.",10,112)
    else
     print("\^ioh. ok.",10,112)
    end
   end
  end
  if dend then
   ddialog,dend,title=false,false,1
  else
   return
  end
 end
 if (title==2) then
  cls()
  spr(128,0,16,16,8)
--  print("\^w\^tgrand",48,28,2)
--  print("\^w\^tcloud",48,44,2)
--  print("\^w\^talley",48,60,2)
  print("for rndgame jam ii  #",0,0,6)
  s="rndgame2021"
  for i=1,#s do
   print(sub(s,i,i),(i+20)*4,0,flr(rnd(15))+1)
  end
  print("by impliedchaos",36,84,5)
  s="press 🅾️  or ❎  to start"
  for i=1,#s do
   print(sub(s,i,i),16+i*4,108+sin(i/20+(py&63)/63+0.25)*3,7)
  end
  return
 elseif (title==1) then
  rectfill(0,0,127,127,12)
 end
 -- erase our sprites
 if title==0 then
  spr(128,0,oy,16,1)
  spr(192,0,8,16,1)
  spr(208,0,112,16,1)
  if (dh!=0 and dh!=1) spr(144,ohx,ohy)
 end
 title=0
 
 -- draw 2 pixels of sky
 rect(0,0,1,127,12)

 -- draw oncoming clouds
 if (dc>0) then
   local dx=dc-1
   spr(96+dct*4,0-dx*2,dcy,dc/4,2)
   dc+=1
   if (dc>16) then
     dc=0
     dct+=1
     dct%=4
   end
 end

 -- shift screen left 2px
 for i=1,126 do
  memcpy(0x6000+i*64,0x6001+i*64,64)
 end
 line(0,0,127,0,12)
 line(0,127,127,127,12)
 
 -- draw smoke cloud
 local z=1
 local s=rand(4)+1
 if (smoke and boost>0) s=rand(8)+1
 if (py&1==0) z=-1
 circ(x-4,y+4,s,6)
 circfill(x-4,y+4+z,s,7)

 if (dh==-1) then
  splodey(dhx,dhy)
  dh=0
 end

 if bk>75 and streak<10 then
  local q=bk-74
  if (q>200) q=200
  if (hf>1) hf-=1
  for i=0,q do
   print(fetti[rand(#fetti)],rand(100),rand(110)+8,rand(16))
  end
 end    
 -- save screen to erase sprites on next pass
 memcpy(0x1000,0x6000+y*64,512)
 memcpy(0x1800,0x6200,512)
 memcpy(0x1a00,0x7c00,512)
 if (dh>0) then
  for i=0,7 do
   memcpy(0x1200+i*64,0x6000+(dhy+i)*64+dhx\2,4)
  end
  ohx,ohy=dhx,dhy
 end
 
 -- draw halucination
 if (dh>0) then
  dh+=1
  local hf=(py&4) >> 2
  spr(20+dht*2+hf,dhx,dhy)
 end
 
 -- draw ship
 local frame=py&7
 spr(2+frame*2,x,y,2,1)
 ox,oy=x,y
 -- beam cannon
 if (fire and beam>0) then
   local fr2=py&3
   local pos=x+16
   if (pos<128) then
     spr(64+fr2,pos,y)
     pos+=8
     while (pos<127) do
       spr(80+fr2,pos,y)
       pos+=8
     end
   end
 end

 -- draw cloud alley
 spr(32,py*-1,112,16,2)
 spr(32,128-py,112,16,2)
 spr(32,py*-1,0,16,2,true,true)
 spr(32,128-py,0,16,2,true,true)

 -- draw gauges
 rect(9,1,42,7,3)
 rect(84,1,117,7,2)
 rectfill(10,2,41,6,0x49)
 rectfill(85,2,116,6,0x49)
 if (boost>0) rectfill(10,2,boost/4+10,6,11)
 if (smoke) print("🅾️boost",12,2,7) else print("🅾️boost",12,2,0)
 if (beam>0) rectfill(85,2,beam/4+85,6,8)
 if (fire) print("❎beam",89,2,7) else print("❎beam",89,2,0)

 local s="-=] grand cloud alley [=-"
 local c0,c1,c2,c3,c4,c5=0,8,9,10,11,12
 if (bk>20 and streak<60) c1,c2,c3,c4,c5=8,4,8,4,8
 if (bk>50 and streak<30) then
  s="-=] evil duck massacre [=-"
  c0=15
  pal(15,7,1)
 else
  pal(15,15,1)
 end
 local spx=60-#s*2
 if (bk>30 and streak<50) pal(12,5,1) else pal(12,12,1)
 if (bk>40 and streak<40) then 
  pal(7,14,1)
  pal(6,13,1)
 else
  pal(7,7,1)
  pal(6,6,1)
 end
 for i=1,#s do
  print(sub(s,i,i),spx+i*4,120+sin(i/20+(py&63)/63+0.25)*3,c5)
  print(sub(s,i,i),spx+i*4,120+sin(i/20+(py&63)/63+0.2)*3,c4)
  print(sub(s,i,i),spx+i*4,120+sin(i/20+(py&63)/63+0.15)*3,c3)
  print(sub(s,i,i),spx+i*4,120+sin(i/20+(py&63)/63+0.1)*3,c2)
  print(sub(s,i,i),spx+i*4,120+sin(i/20+(py&63)/63+0.05)*3,c1)
  print(sub(s,i,i),spx+i*4,120+sin(i/20+(py&63)/63)*3,c0)
 end
end

function splodey(x,y)
 x+=4
 y+=4
 circ(x,y,5,6)
 circfill(x,y,4,7)
 for i=0,6 do
  local r=rand(5)+1
  local ox=1
  local oy=1
  if (rand(2)==0) ox=-1 
  if (rand(2)==0) oy=-1 
  local t=rnd(1)
  circ(x+ox+sin(t)*r,y+oy+cos(t)*r,r,6)
  circfill(x+sin(t)*r,y+cos(t)*r,r,7)
 end
 if bk>50 then
  for i=0,24 do
   line(x,y,x+cos(rnd(1))*rand(24),y+sin(rnd(1))*rand(24),8)
  end
 end   
end

function thehaps()
 dp,dy,ddialog=0,0,true
end

function helpme()
 dp,dy,ddialog=2,0,true
end

function rand(x)
  return flr(rnd(x))
end

