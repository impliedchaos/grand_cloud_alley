pico-8 cartridge // http://www.pico-8.com
version 32
__lua__
function _init()
 py=0
 x,y,ox,oy=64,64,64,64
 smoke,fire=false,false
 boost,beam=127,127
 dc,dct,dcy=0,0,0
 rectfill(0,0,127,127,12)
 sfx(0,3)
end

function _update()
 py+=1
 py%=128
 if (btn(5)) fire=true else fire=false
 if (btn(4)) smoke=true else smoke=false
 if (smoke and boost>0) then
  boost-=1
  if (btn(0)) x-=5
  if (btn(1)) x+=5
  if (btn(2)) y-=5
  if (btn(3)) y+=5
  sfx(1,3)
 else 
  if (btn(0)) x-=3
  if (btn(1)) x+=3
  if (btn(2)) y-=3
  if (btn(3)) y+=3
  sfx(0,3)
 end
 if (not smoke and boost<127) boost+=1
 if (fire and beam>0) then
  beam-=1
  sfx(2,2)
 else
  sfx(-2,2)
 end
 if (not fire and beam<127) beam+=1
 if (x<0) x=0
 if (x>120) x=120
 if (y<0) y=0
 if (y>120) y=120
 if (dc==0) then
  local z=flr(rnd(10))
  if (z==0) then
   dc=1
   dcy=rnd(90)+10
  end
 end
end

function _draw()
 -- erase our sprites
 spr(128,0,oy,16,1)
 spr(192,0,8,16,1)
 spr(208,0,112,16,1)

 -- draw 2 pixels of sky
 rect(0,0,1,127,12)

 -- draw oncoming clouds
 if (dc>0) then
   local dx=dc-1
   spr(96+dct*4,0-dx*2,dcy,dc/4,2)
   dc+=1
   if (dc>16) then
     dc=0
     dct+=1
     dct%=2
   end
 end

 -- shift screen left 2px
 for i=1,127 do
  memcpy(0x6000+i*64,0x6001+i*64,64)
 end
 
 -- draw smoke cloud
 local z=1
 local s=rnd(4)+1
 if (smoke and boost>0) s=rnd(8)+1
 if (py&1==0) z=-1
 circ(x-4,y+4,s,6)
 circfill(x-4,y+4+z,s,7)

 -- save screen to erase sprites on next pass
 memcpy(0x1000,0x6000+y*64,512)
 memcpy(0x1800,0x6200,512)
 memcpy(0x1a00,0x7c00,512)

 -- draw ship
 local frame=py&7
 spr(2+frame*2,x,y,2,1)
 ox,oy=x,y
 -- beam cannon
 if (fire and beam>0) then
   local fr2=py&3
   local pos=x+16
   if (pos<127) then
     spr(64+fr2,pos,y)
     pos+=8
     while (pos<127) do
       spr(80+fr2,pos,y)
       pos+=8
     end
   end
 end

 -- draw clouds
 spr(32,py*-1,112,16,2)
 spr(32,128-py,112,16,2)
 spr(32,py*-1,0,16,2,true,true)
 spr(32,128-py,0,16,2,true,true)

 -- draw gauges
 rect(9,1,42,7,3)
 rect(84,1,117,7,2)
-- fillp(0b0101101001011010)
 rectfill(10,2,41,6,0x49)
 rectfill(85,2,116,6,0x49)
 fillp()
 if (boost>0) rectfill(10,2,boost/4+10,6,11)
 if (smoke) print("üÖæÔ∏èboost",12,2,7) else print("üÖæÔ∏èboost",12,2,0)
 if (beam>0) rectfill(85,2,beam/4+85,6,8)
 if (fire) print("‚ùébeam",89,2,7) else print("‚ùébeam",89,2,0)

 local s="-=] grand cloud alley [=-"
-- print("-=] grand cloud alley [=-",15,122,0)
 for i=1,#s do
  print(sub(s,i,i),10+i*4,120+sin(i/20+(py&63)/63+0.2)*3,11)
  print(sub(s,i,i),10+i*4,120+sin(i/20+(py&63)/63+0.15)*3,10)
  print(sub(s,i,i),10+i*4,120+sin(i/20+(py&63)/63+0.1)*3,9)
  print(sub(s,i,i),10+i*4,120+sin(i/20+(py&63)/63+0.05)*3,8)
  print(sub(s,i,i),10+i*4,120+sin(i/20+(py&63)/63)*3,0)
 end
end
